<inherit template='flashcard\root' ext='xml'>
	<var name='message' value='Welcome'></var>
	<node type='div' override='content'>
		<node type='div' _id='cards'>
			<t>CARDS</t>
			<node type='div' _class='tab-container'>
				<node type='ul'>
					<node type='li'>
						<t>Word</t>
					</node>
					<node type='li'>
						<t>Meaning</t>
					</node>
					<node type='li'>
						<t>Example</t>
					</node>
				</node>
			</node>
			<node type='div' _class='card-container'>
				<node type='ul' _class='star-list'></node>
				<node type='div' _class='word-container'></node>
			</node>
			<node type='ul' _class='handlers'>
				<node type='li'>
					<t>Next</t>
				</node>
			</node>
		</node>
	</node>
	<node type='script' _src='staticcontent/scripts/library/jquery/jquery-min'></node>
	<node type='script' _src='staticcontent/scripts/library/flashcard/FlashService'></node>
	<node type='script' _src='staticcontent/scripts/library/flashcard/main'></node>
</inherit>