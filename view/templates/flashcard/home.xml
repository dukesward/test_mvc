<inherit template='flashcard\root' ext='xml'>
	<var name='message' value='Welcome'></var>
	<node type='div' override='content'>
		<node type='div' _id='cards'>
			<t>CARDS</t>
			<node type='div' _class='tab-container'>
				<node type='ul'>
					<node type='li' _class='active' _util='word'>
						<t>Word</t>
					</node>
					<node type='li' _util='meaning'>
						<t>Meaning</t>
					</node>
					<node type='li' _util='example'>
						<t>Example</t>
					</node>
				</node>
			</node>
			<node type='div' _class='card-container'>
				<node type='ul' _class='star-list'></node>
				<node type='div' _class='type'></node>
				<node type='div' _class='word-container word'></node>
				<node type='div' _class='word-container meaning'>
					<node type='div' _class='wrapper'></node>
				</node>
				<node type='div' _class='word-container example'>
					<node type='div' _class='wrapper'></node>
				</node>
				<node type='div' _class='exp-container'>
					<node type='div' _class='wrapper'>
						<node type='span' _class='exp title'>
							<t>EXP: </t>
						</node>
						<node type='span' _class='exp current'></node>
						<node type='span'>
							<t>/</t>
						</node>
						<node type='span' _class='exp next'></node>
					</node>
				</node>
			</node>
			<node type='ul' _class='handlers'>
				<node type='li' _util='next'>
					<t>Next</t>
				</node>
				<node type='li' _util='earn'>
					<t>Earn Exp</t>
				</node>
				<node type='li' _util='add'>
					<t>Add</t>
				</node>
			</node>
		</node>
	</node>
	<static type='script' name='global'></static>
</inherit>